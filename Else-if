package day3;
import java.util.Scanner;
public class Elseif{
		    public static void main(String[] args) {
		    Scanner sc=new Scanner(System.in);
		    int num=sc.nextInt();
		        if (num % 3 == 0 && num % 5 ==0) {
		            System.out.println("fizzbizz");
		        }
		        else if (num % 5 == 0) {
		            System.out.println("bizz");
		        }
		        else if (num % 3 == 0 && num % 5 == 0) {
		            System.out.println("fizz and bizz");
		        }
		    }
}
		
